{
  "schemaVersion": 1,
  "id": "multi-instance",
  "version": "${version}",
  "name": "Multi Instance",
  "description": "Instancing in a multiple way.",
  "authors": [
    "Crosby"
  ],
  "contact": {
    "repo": "https://github.com/RacoonDog/multi-instance-meteor",
    "sources": "https://github.com/RacoonDog/multi-instance-meteor",
    "issues": "https://github.com/RacoonDog/multi-instance-meteor/issues"
  },
  "icon": "assets/multi-instance/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.multiinstance.MultiInstance"
    ]
  },
  "mixins": [
    "multi-instance.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "73,151,208"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
